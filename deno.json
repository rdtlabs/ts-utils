{
  "name": "@rdtlabs/ts-utils",
  "version": "0.2.33",
  "exports": {
    ".": "./src/index.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "src/**/*.ts",
      "deno.json"
    ],
    "exclude": [
      ".github/",
      ".vscode/",
      "dist/",
      "build*",
      "package/",
      "tsconfig.ts",
      "tsup.config.ts",
      ".npmignore",
      ".gitignore",
      ".editorconfig",
      ".gitattributes",
      "node_modules/",
      "main.local.ts",
      ".vscode/",
      "src/**/*.test.ts"
    ]
  },
  "tasks": {
  },
  "imports": {
    "@std/": "https://deno.land/std@0.224.0/"
  },
  "compilerOptions": {
    "lib": [
      "deno.ns",
      "dom"
    ]
  },
  "lint": {
    "include": ["src/"],
    "exclude": ["dist/", "node_modules/", ".vscode/", "src/**/*.test.ts"],
    "rules": {
      "include": [
        "eqeqeq",
        "explicit-module-boundary-types",
        "no-eval",
        "no-undef",
        "verbatim-module-syntax"
      ],
      "tags": [
        "recommended"
      ]
    }
  },
  "fmt": {
    "include": ["src/"],
    "exclude": ["dist/", "node_modules/", ".vscode/", "src/**/*.test.ts"]
  }
}
